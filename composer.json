{
    "name": "omed/laravel-security",
    "description": "Security package for Omed",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Omed\\Component\\Security\\": "./"
        }
    },
    "require": {
        "omed/orm-component": "^1.0@dev",
        "symfony/security-core": "^3.4|^4.0|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5.8"
    }
}
